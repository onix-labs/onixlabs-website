<Router AppAssembly="@typeof(App).Assembly">
    <Found Context="routeData">
        <RouteView RouteData="@routeData" DefaultLayout="@typeof(LayoutRoot)"/>
        @* <FocusOnNavigate RouteData="@routeData" Selector="h1"/> *@
    </Found>
    <NotFound>
        <PageTitle>Not found</PageTitle>
        <LayoutView Layout="@typeof(LayoutRoot)">
            <NotFound/>
        </LayoutView>
    </NotFound>
</Router>